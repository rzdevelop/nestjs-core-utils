{
  "name": "nestjs-core-utils",
  "version": "0.0.9",
  "description": "Nestjs Cire Utils Package",
  "author": "rzdev",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzdevelop/nestjs-core-utils.git"
  },
  "bugs": {
    "url": "https://github.com/rzdevelop/nestjs-core-utils/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/rzdevelop/nestjs-core-utils#readme",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "predeploy": "npm version patch",
    "deploy": "npm publish",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "test:cov": "jest --config jestconfig.json --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/terminus": "^7.1.0",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.0",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "25.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
}
